variable "encoding" {
  type        = string
  description = "Encoding Type"
  default = "SSH"
}

variable "username" {
  type        = string
  description = "Encoding Type"
  default = "default-user"
}


variable "public_key" {
  type        = string
  description = "Encoding Type"
  default = "ssh-rsa  mytest@mydomain.com"
}
